{
  "name": "electron-webpack-forge",
  "version": "6.0.4",
  "description": "Webpack plugin for Electron Forge, lets you use Webpack directly in your tooling",
  "license": "MIT",
  "main": "dist/WebpackPlugin.js",
  "typings": "dist/WebpackPlugin.d.ts",
  "scripts": {
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "node tools/cleanup && tsc --project tsconfig.json",
    "package": "npm run build && npm pack",
    "eslint": "eslint --ext .ts src/",
    "eslint-fix": "eslint --fix  --ext .ts src/"
  },
  "devDependencies": {
    "@malept/cross-spawn-promise": "^2.0.0",
    "@types/debug": "^4.1.7",
    "@types/express-ws": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "chai": "^4.3.7",
    "electron-packager": "^17.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1",
    "typescript": "^4.9.4",
    "which": "^3.0.0",
    "xvfb-maybe": "^0.2.1"
  },
  "engines": {
    "node": ">= 14.17.5"
  },
  "dependencies": {
    "@electron-forge/core-utils": "^6.0.4",
    "@electron-forge/plugin-base": "6.0.4",
    "@electron-forge/shared-types": "6.0.4",
    "@electron-forge/web-multi-logger": "6.0.4",
    "chalk": "4.1.2",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
