{
  "name": "electron-wp",
  "version": "0.1.0",
  "description": "Webpack plugin for Electron Forge, lets you use Webpack directly in your tooling",
  "license": "MIT",
  "main": "dist/WebpackPlugin.js",
  "typings": "dist/WebpackPlugin.d.ts",
  "scripts": {
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "node tools/cleanup && tsc --project tsconfig.json",
    "package": "npm run build && npm pack"
  },
  "devDependencies": {
    "@malept/cross-spawn-promise": "^2.0.0",
    "@types/debug": "^4.1.7",
    "@types/express-ws": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.3",
    "chai": "^4.3.3",
    "electron-packager": "^15.4.0",
    "mocha": "^9.0.1",
    "sinon": "^13.0.1",
    "tsc": "^2.0.4",
    "typescript": "^4.8.3",
    "which": "^2.0.2",
    "xvfb-maybe": "^0.2.1"
  },
  "engines": {
    "node": ">= 14.17.5"
  },
  "dependencies": {
    "@electron-forge/async-ora": "6.0.0-beta.66",
    "@electron-forge/core": "6.0.0-beta.66",
    "@electron-forge/plugin-base": "6.0.0-beta.66",
    "@electron-forge/shared-types": "6.0.0-beta.66",
    "@electron-forge/web-multi-logger": "6.0.0-beta.66",
    "chalk": "^4.0.0",
    "debug": "^4.3.4",
    "fs-extra": "^10.0.0",
    "global": "^4.3.2",
    "html-webpack-plugin": "^5.3.1",
    "webpack": "^5.69.1",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.7.3"
  }
}
